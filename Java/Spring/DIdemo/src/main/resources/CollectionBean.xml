<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="javaCollection" class="com.edgmed.DIdemo.InjCollection.JavaCollection">
        <property name="injectionList">
            <list>
                <value>1</value>
                <value>2</value>
                <value>3</value>
                <value>4</value>
            </list>
        </property>

        <property name="injectionSet">
            <set>
                <value>1A</value>
                <value>2A</value>
                <value>3A</value>
                <value>3A</value>
            </set>
        </property>

        <property name="injectionMap">
            <map>
                <entry key="1" value="bork1"/>
                <entry key="2" value="bork2"/>
                <entry key="3" value="bork3"/>
                <entry key="4" value="bork4"/>
            </map>
        </property>

        <property name="injectionProperties">
            <props>
                <prop key="1" >Aux1</prop>
                <prop key="2" >Aux2</prop>
                <prop key="3" >Aux3</prop>
                <prop key="4" >Aux4</prop>
            </props>
        </property>
    </bean>

</beans>